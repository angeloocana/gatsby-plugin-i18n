{
  "name": "gatsby-plugin-i18n-tags",
  "version": "1.0.0",
  "description": "Multi language feature for Gatsby",
  "homepage": "https://github.com/angeloocana/gatsby-plugin-i18n-tags#readme",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix ./src",
    "prebuild": "yarn run lint",
    "build": "babel src --out-dir .",
    "watch": "babel -w src --out-dir .",
    "pretest": "yarn run build",
    "test": "jest",
    "coverage": "codecov"
  },
  "respository": {
    "type": "git",
    "url": "https://github.com/angeloocana/gatsby-plugin-i18n-tags"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n"
  ],
  "author": "angeloocana.com",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.7.2",
    "graphql": "^14.5.8",
    "ptz-i18n": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "ptz-assert": "^1.6.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "readme": "README.md",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
